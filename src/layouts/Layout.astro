---
import "../styles/global.css";
const { title } = Astro.props;
import Nav from "../components/Nav.astro";
---

<!doctype html>
<html lang="de">
  <head>
    <meta charset="UTF-8" />
    <title>{title}</title>
  </head>
  <body class="font-sans bg-black text-white grid grid-cols-12">
    <div class="col-span-6 col-start-4">
      <Nav />
      <slot />
    </div>
  </body>
</html>
